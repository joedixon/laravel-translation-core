{
    "name": "joedixon/laravel-translation-core",
    "description": "A library for interacting with translations in a Laravel project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joe Dixon",
            "email": "hello@joedixon.co.uk"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/filesystem": "^9.0|^10.0",
        "symfony/var-dumper": "^6.2",
        "illuminate/http": "^10.10"
    },
    "require-dev": {
        "mockery/mockery": "^1.0.0",
        "pestphp/pest": "^2.0",
        "orchestra/testbench": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "JoeDixon\\TranslationCore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoeDixon\\TranslationCore\\TranslationServiceProvider"
            ]
        }
    }
}